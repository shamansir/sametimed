<?xml version="1.0" encoding="UTF-8"?>
<waveserver>
	<!-- these setting must conform with settings in run-config.sh of 
	     Federation Wave Protocol Server -->
	<!-- equals to WAVE_SERVER_DOMAIN_NAME in run-config.sh -->
	<domain-name>acmewave.com</domain-name>
	<!-- equals to WAVE_SERVER_HOSTNAME in run-config.sh  -->	
	<hostname>localhost</hostname>
	<!-- equals to WAVE_SERVER_PORT in run-config.sh -->
	<port>9876</port>
</waveserver>
