<?xml version="1.0" encoding="UTF-8"?>
<sametimed>
		
	<service>

        <app-name>sametimed</app-name>
        				
		<tunnel>
		    <path>/w</path>
            <channels>
        		<join-channel>/join</join-channel>
        		<confirm-channel>/confirm</confirm-channel>
        		<commands-channel>/command</commands-channel>
        		<updates-channel>/update</updates-channel>
    		</channels>
		</tunnel>
		
        <cometd-init>/cometd</cometd-init>
        				
	</service>
	
	<prepared-modules>
		<module id="inbox" />
        <module id="errors" />
        <module id="users" />
        <module id="console" />
        <module id="example" />        
        <module id="chat" package="name.shamansir.sametimed.modules.chat" />                              
	</prepared-modules>
	
	<disabled-modules>
		<module-id>example</module-id>	
	</disabled-modules>
	
	<registered-commands>
		<command alias="new" id="CMD_NEW" />
        <command alias="open" id="CMD_OPEN" />
        <command alias="add" id="CMD_ADD" />
        <command alias="undo" id="CMD_UNDO" />
        <command alias="remove" id="CMD_REMOVE" />
        <command alias="swch" id="CMD_VIEW" />               		
	</registered-commands>
	
</sametimed>