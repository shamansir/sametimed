<?xml version="1.0" encoding="UTF-8"?>
<sametimed>
		
	<service>

        <app-name>sametimed</app-name>
        				
		<tunnel>w</tunnel>
        <channels>
    		<join-channel>/join</join-channel>
    		<cmd-channel>/cmd</cmd-channel>
    		<upd-channel>/upd</upd-channel>
		</channels>
		
        <cometd-init>cometd</cometd-init>
        				
	</service>
	
	<enabled-modules>
		<module id="inbox"   system="true" />
        <module id="errors"  system="true" />
        <module id="users"   system="true" />
        <module id="console" system="true" />
        <module id="example" />        
        <module id="chat"    path="name.shamansir.sametimed.modules" />
        <!--
        <module id="calendar" path="com.slurm.module" />
        <module id="editor" />
        -->                                
	</enabled-modules>
	
	<registered-commands>
		<command alias="new"  system="true" />
        <command alias="open" system="true" />
        <command alias="add"  system="true" />
        <command alias="undo" system="true" />                
        <command alias="swch" system="true" id="CMD_VIEW" />
        <command alias="say"  defined-for="chat" />
        <!--
        <command alias="CMD_PUT" defined-for="editor" />
        <command alias="CMD_SETDATE" defined-for="calendar" />        
        -->                		
	</registered-commands>
	
</sametimed>